{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "assets": [
      {
        "include": "sub_modules/entity/src/**/*",
        "exclude": "(sub_modules/src/{main,config,migration}.ts)|(sub_modules/src/migrations)",
        "watchAssets": true
      }
    ]
  }
}
